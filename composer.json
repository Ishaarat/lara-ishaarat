{
    "name": "ishaarat/lara-ishaarat",
    "description": "Send Whatsapp notifications from your laravel application using Ishaarat",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "keywords": [
        "ishaarat",
        "whatsapp",
        "laravel whatsapp gateway",
        "laravel twilio",
        "laravel link mobility",
        "laravel ishaarat"
    ],
    "homepage": "https://ishaarat.com/",
    "autoload": {
        "psr-4": {
            "Ishaarat\\LaraIshaarat\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "authors": [
        {
            "name": "Ishaarat Tech Team",
            "email": "ishaaratcom@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/http": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ishaarat\\LaraIshaarat\\Providers\\IshaaratServiceProvider"
            ],
            "aliases": {
                "Ishaarat": "Ishaarat\\LaraIshaarat\\Facades\\WA"
            }
        }
    }
}
